<%= form_for @user, url: {action: "update"} do |f| %>
  <%= f.label :username%>
  <%= f.text_field :username, label: "Username" %><br/>
  <%= f.label :email%>
  <%= f.email_field :email %><br/>
  <%= f.label :password%> 
  <%= f.text_field :password %><br/>
  <%= f.submit "Update" %>
<% end %>
  